<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= homeContent %></p>

<% posts.forEach(post => { %>
     <a href=<%= "post/"+post.title %> >
         <h1><%= post.title %></h1>
    </a>
     <p><%= post.body.substring(0, 280) %>...</p>
     <a title="Ir para postagem" href=<%= "post/"+post.title %>>Saiba mais</a>
    <% }) %>

<%- include("partials/footer") -%>